<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>CoachEngine</servlet-name>
		<servlet-class>charbon_burguburu.CoachEngineServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>DescriptionCache</servlet-name>
		<servlet-class>charbon_burguburu.DescriptionCache</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CoachEngine</servlet-name>
		<url-pattern>/coachengine</url-pattern>
	</servlet-mapping>
	<servlet>
	    <servlet-name>Success</servlet-name>
	    <servlet-class>charbon_burguburu.Success</servlet-class>
	</servlet>
	<servlet>
	    <servlet-name>Login</servlet-name>
	    <servlet-class>charbon_burguburu.Login</servlet-class>
	</servlet>
	<servlet-mapping>
	    <servlet-name>Success</servlet-name>
	    <url-pattern>/success</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DescriptionCache</servlet-name>
		<url-pattern>/descriptioncache</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
	    <servlet-name>Login</servlet-name>
	    <url-pattern>/login</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
